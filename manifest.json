{
  "manifest_version": 3,
  "name": "Opinionated Extension",
  "description": "For personal use, if your not me, you probably don't want this.",
  "version": "0.5",
  "permissions": ["tabs", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "new_tab.html"
  },
  "icons": {
    "16": "assets/icon-16.png"
  },
  "commands": {
    "next-tab": {
      "suggested_key": {
        "default": "Ctrl+Shift+L",
        "mac": "MacCtrl+Shift+L"
      },
      "description": "Move to the next tab"
    },
    "previous-tab": {
      "suggested_key": {
        "default": "Ctrl+Shift+H",
        "mac": "MacCtrl+Shift+L"
      },
      "description": "Move to the previous tab"
    }
  }
}
