{
  "manifest_version": 3,
  "name": "QOL",
  "description": "For personal use, if your not me, you probably don't want this.",
  "version": "0.5",
  "permissions": ["tabs", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "assets/icon-16.png"
  },
  "commands": {
    "next-tab": {
      "suggested_key": {
        "default": "Ctrl+Shift+L",
        "mac": "Command+Shift+L"
      },
      "description": "Move to the next tab"
    },
    "previous-tab": {
      "suggested_key": {
        "default": "Ctrl+Shift+H",
        "mac": "Command+Shift+H"
      },
      "description": "Move to the previous tab"
    },
    "new-tab-to-the-right": {
      "suggested_key": {
        "default": "Alt+Shift+T",
        "mac": "Alt+Shift+T"
      },
      "description": "Open a new tab to the right of the current tab"
    },
    "chrome-new-tab": {
      "suggested_key": {
        "default": "Alt+T",
        "mac": "Alt+T"
      },
      "description": "Navigate to the new tab page"
    }
  }
}
